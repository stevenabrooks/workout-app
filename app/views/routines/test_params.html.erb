<%= form_tag routines_path %>

  <%= label_tag "routine[name]", "Routine Name" %>
  <%= text_field_tag "routine[name]" %><br />
    <% 2.times do  %>
      <%= collection_select("lifts", :exercise_id, Exercise.all, :id, :name) %><br />

      <% 2.times do %>
        <%= label_tag "lifts[infos][][weight]", "weight" %>
        <%= text_field_tag "lifts[infos][][weight]" %>
        
        <%= label_tag "lifts[infos][][repetition]", "reps" %>
        <%= text_field_tag "lifts[infos][][repetition]" %><br />
      <% end %>
    <% end %>
  

<%= submit_tag "Submit Routine" %>

